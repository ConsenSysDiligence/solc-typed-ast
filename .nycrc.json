{
    "extension": [".ts"],
    "exclude": [
        "**/*.d.ts",
        "**/node_modules/**",
        "**/test/**",
        "**/coverage/**",
        "**/docs/**",
        "**/.compiler_cache/**",
        "src/compile/inference/file_level_definitions_parser*.ts",
        "src/ast/comments/comments_parser*.ts"
    ],
    "reporter": ["lcov", "text-summary"],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "branches": 85,
    "functions": 90,
    "lines": 95
}
